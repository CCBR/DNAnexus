{
  "name": "ccbr_cutadapt_1.18_pe",
  "title": "CCBR cutadapt PE",
  "summary": "trim paired end reads",
  "dxapi": "1.0.0",
  "version": "0.0.2",
  "inputSpec": [
    {
      "name": "InFq1",
      "label": "Input Fastq Read1",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.fastq.gz",
        "*.fastq"
      ],
      "help": ""
    },
    {
      "name": "InFq2",
      "label": "Input Fastq Read2",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.fastq.gz",
        "*.fastq"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "OutFq1",
      "label": "Output Fastq Read1",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "OutFq2",
